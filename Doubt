class ineuron:
    def __init__(self,specialisation,batch):
        self.specialisation= specialisation
        self.batch= batch

        self._batch_id = "D0010"
        self.__student_id = "FSDS123"
    def detail1(self):
        print("Specialisation: ",self.specialisation)
        print("Batch: ", self.batch)

class student_details(ineuron):
    def __init__(self,name,place):
        self.name = name
        self.place= place

    def details(self):
        print("Student name:",self.name)
        print("Place: ",self.place)
        print("Specialisation: ",self.specialisation)
        print("Batch: ",self.batch)

d = ineuron("FSDS","A02")
d.details()
s = student_details("Hari","Mumbai")
s.details()
